##############################
# Environment Configuration   #
# Rename to `.env` and fill   #
# in secrets before running.  #
##############################

# Core runtime configuration
# RECOMMENDED: Use OpenRouter or AI/ML API for access to 200+ models
PROVIDER=openrouter
MODEL=openai/gpt-4-turbo

# Alternative providers:
# PROVIDER=aimlapi
# MODEL=gpt-4o-mini
# PROVIDER=google
# MODEL=gemini-2.5-flash
# PROVIDER=openai
# MODEL=gpt-4-turbo

# API keys (set only what you need)
OPENROUTER_API_KEY=
AIMLAPI_API_KEY=
GEMINI_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
DEEPSEEK_API_KEY=

# Provider-specific overrides (optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
VERTEX_PROJECT_ID=
VERTEX_LOCATION=
VERTEX_MODEL=

# Optional run-time tuning
WANT_LOGPROBS=19
POOL_MODE=lse
TAU=2.0
TEMPERATURE=0.0
TOP_P=1.0
SEED=42

